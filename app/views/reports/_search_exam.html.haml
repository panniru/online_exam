= form_tag grouped_results_reports_path, :method => 'get' , :class => "form-inline", :id => "exam_results_report" do
  .col-md-12
    = label_tag :exam, "Exam",:class => 'col-md-1 control-label'
    .col-md-6
      = select_tag :exam_id, options_from_collection_for_select(@user_exams, "id", "exam_full_name", params[:exam_id]),  :class => 'form-control'
    .col-md-4
      = submit_tag "Get Report", :name => nil, :class => "btn-primary btn"
      = link_to "Print Report", "#", :class => "btn-primary btn", :rel => "print-report"
