%table.table.table-bordered.table-hover
  %thead
    %th Roll No
    %th Name
    %th Course
    %th Year
    %th Semester
    %th email
  %tbody
    - students.each do |student|
      %tr
        %td= link_to student.roll_number, student
        %td= student.name
        %td= student.course_name
        %td= student.year
        %td= student.semester
        %td= student.user.try(:email)
