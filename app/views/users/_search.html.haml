= form_tag users_path, :method => 'get' , :class => "form-inline" do
  .form-group.col-md-6
    = autocomplete_field_tag :search_term,
    '',
    auto_search_autocomplete_user_user_info_path,
    :placeholder => "User Id", :class => 'form-control',
    :id_element => "#search"
    = hidden_field_tag :search, :id => "search"
  = submit_tag "Search", :name => nil, :class => "btn-primary btn"
  = link_to "Add Admin", new_user_path, :class => "btn btn-primary"
